{
  "manifest_version": 2,
  "name": "Asana Tab",
  "version": "1.0",

  "chrome_url_overrides" : {
    "newtab": "index.html"
  },

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },

  "permissions": [
    "storage",
    "cookies",
    "http://*/*",
    "https://*/*"
  ],

  "content_scripts": [
    {
      "matches": ["http://www.google.com/*"],
      "js": ["js/main.js"]
    }
  ]
}
